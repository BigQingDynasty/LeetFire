class MyHashSet {

    private var data: [Bool]

    init() {
        data = [Bool](repeating: false, count: 1000001)
    }
    
    func add(_ key: Int) {
        data[key] = true
    }
    
    func remove(_ key: Int) {
        data[key] = false
    }
    
    func contains(_ key: Int) -> Bool {
        data[key]
    }
}
